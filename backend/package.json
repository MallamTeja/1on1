{
  "name": "scholarsearch-backend",
  "version": "1.0.0",
  "description": "Backend services for ScholarSearch",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --watch src --ext ts,json --exec \"ts-node -P ../tsconfig.backend.json src/index.ts\"",
